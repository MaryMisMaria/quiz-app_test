{
  "compilerOptions": {
    "noEmit": true,
    "module": "esnext",
    "target": "es5",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "typeRoots": ["node_modules/@types"],
    "sourceMap": false,
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "resolveJsonModule": true,
    "noImplicitAny": false
  },
  "exclude": ["node_modules"]
}
